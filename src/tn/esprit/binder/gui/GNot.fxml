<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="455.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.binder.gui.GNotController">
   <children>
      <Pane prefHeight="83.0" prefWidth="700.0" style="-fx-background-color: #48d1cc  #48d1cc;">
         <children>
            <Label layoutX="206.0" layoutY="22.0" text="Notifications Management">
               <font>
                  <Font name="JasmineUPC" size="41.0" />
               </font>
            </Label>
            <ImageView fitHeight="72.0" fitWidth="191.0" layoutX="7.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@binder.PNG" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <TableView fx:id="tablenotification" layoutX="132.0" layoutY="127.0" prefHeight="309.0" prefWidth="300.0">
         <columns>
            <TableColumn fx:id="category" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="sender" prefWidth="75.0" text="Sender" />
            <TableColumn fx:id="receiver" prefWidth="75.0" text="Receiver" />
            <TableColumn fx:id="content" prefWidth="75.0" text="Content" />
         </columns>
      </TableView>
      <Button fx:id="btnadd" layoutX="468.0" layoutY="158.0" mnemonicParsing="false" onAction="#btnAdd" prefHeight="6.0" prefWidth="168.0" style="-fx-background-color: #48d1cc  #48d1cc;" text="ADD">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <Button fx:id="btndelete" layoutX="468.0" layoutY="236.0" mnemonicParsing="false" onAction="#btnDelete" prefHeight="12.0" prefWidth="168.0" style="-fx-background-color: #48d1cc;" text="DELETE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <Button fx:id="home" layoutX="468.0" layoutY="282.0" mnemonicParsing="false" onAction="#btnhome" prefHeight="25.0" prefWidth="168.0" style="-fx-background-color: #dc143c;" text="Go Back">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <Button fx:id="btnupdate" layoutX="468.0" layoutY="194.0" mnemonicParsing="false" onAction="#btnUpdate" prefHeight="6.0" prefWidth="168.0" style="-fx-background-color: #48d1cc  #48d1cc;" text="UPDATE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <ImageView fitHeight="46.0" fitWidth="40.0" layoutX="468.0" layoutY="189.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@update.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="40.0" fitWidth="40.0" layoutX="468.0" layoutY="231.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@delete.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="40.0" fitWidth="40.0" layoutX="468.0" layoutY="277.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@goBack%20.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="49.0" fitWidth="40.0" layoutX="468.0" layoutY="153.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@add.png" />
         </image>
      </ImageView>
   </children>
